{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Paxeer Network Documentation",
  "colors": {
    "primary": "#FF4500",
    "light": "#FF6B35",
    "dark": "#E63E00"
  },
  "favicon": "/favicon.svg",
  "metadata": {
    "og:title": "Paxeer Network - Developer Documentation",
    "og:description": "Build on Paxeer Network - A high-performance, developer-friendly blockchain platform",
    "og:image": "/og-image.png"
  },
  "seo": {
    "indexHiddenPages": false,
    "metatags": {
      "og:type": "website",
      "twitter:card": "summary_large_image"
    }
  },
  "modeToggle": {
    "default": "dark",
    "isHidden": false
  },
  "contextual": {
    "options": [
      "copy",
      "chatgpt",
      "claude"
    ]
  },
  "feedback": {
    "thumbsRating": true,
    "suggestEdit": true,
    "raiseIssue": true
  },
  "footer": {
    "socials": {
      "discord": "https://discord.gg/paxeer",
      "twitter": "https://twitter.com/paxeernetwork",
      "github": "https://github.com/paxeer"
    }
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Getting Started",
        "groups": [
          {
            "group": "Introduction",
            "pages": [
              "index",
              "quickstart"
            ]
          },
          {
            "group": "Core Concepts",
            "pages": [
              "configuration",
              "contracts",
              "api-reference",
              "rpc"
            ]
          }
        ]
      },
      {
        "tab": "Developers",
        "groups": [
          {
            "group": "Build",
            "pages": [
              "examples",
              "tools"
            ]
          },
          {
            "group": "Ecosystem Protocols",
            "pages": [
              "paxdex",
              "lending",
              "ctm"
            ]
          },
          {
            "group": "APIs",
            "pages": [
              "blockscout-api"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Explorer",
          "href": "https://scan.paxeer.app",
          "icon": "magnifying-glass"
        },
        {
          "anchor": "PaxDex",
          "href": "https://dex.paxeer.app",
          "icon": "arrow-right-arrow-left"
        },
        {
          "anchor": "Discord",
          "href": "https://discord.gg/paxeer",
          "icon": "discord"
        },
        {
          "anchor": "GitHub",
          "href": "https://github.com/paxeer",
          "icon": "github"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Website",
        "href": "https://paxeer.app"
      },
      {
        "label": "Community",
        "href": "https://paxeer.app/community"
      },
      {
        "label": "Support",
        "href": "https://discord.gg/paxeer"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Get Started",
      "href": "/quickstart"
    }
  }
}
